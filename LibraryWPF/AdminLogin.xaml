<Page x:Class="LibraryWPF.AdminLogin"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:LibraryWPF"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AdminLogin">

    <Grid>
        <TextBox x:Name="Admin" HorizontalAlignment="Left" Height="23" Margin="366,88,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TabIndex="1"/>
        <TextBlock HorizontalAlignment="Left" Margin="308,89,0,0" TextWrapping="Wrap" Text="ID" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="290,136,0,0" TextWrapping="Wrap" Text="Password" VerticalAlignment="Top"/>
        <Button IsDefault="True" x:Name="Loginbtn" Content="Login" HorizontalAlignment="Left" Margin="328,195,0,0" VerticalAlignment="Top" Width="75" Click="Loginbtn_Click" TabIndex="3"/>
        <PasswordBox x:Name="AdminPassword" HorizontalAlignment="Left" Margin="366,134,0,0" VerticalAlignment="Top" Width="120" TabIndex="2"/>

    </Grid>
</Page>
